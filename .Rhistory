p10<-0.20
X1<-53
n1<-222
p1bar<-X1/n1
z1<-(p1bar-p10)/sqrt(p10*(1-p10)/n1)
sprintf("The value of the z statistic is: %f", z1)
p1_value<-1-pnorm(z1, 0, 1)
sprintf("The p-value of the test is: %f", p1_value)
prop.test(x=53, n=222, p=0.20, alternative="greater")
